
# Linode API token used to create infrastructure
linode_token = ""

# Admin password to use for Rancher server bootstrap, min. 12 characters
rancher_server_admin_password = ""

# Version of cert-manager to install alongside Rancher (format: 0.0.0)
cert_manager_version = "1.10.0"

# Linode region used for all resources
linode_region = "eu-central"

# Linode type used for all droplets
linode_type = "g6-standard-2"

# Prefix added to names of all resources
prefix = "quickstart"

# The helm repository, where the Rancher helm chart is installed from
rancher_helm_repository = "https://releases.rancher.com/server-charts/latest"

# Kubernetes version to use for Rancher server cluster
rancher_kubernetes_version = "v1.23.14+k3s1"

# Rancher server version (format: v0.0.0)
rancher_version = "2.7.1"

# Kubernetes version to use for managed workload cluster
workload_kubernetes_version = "v1.23.14+rke2r1"